{
  "name": "PWA_Boilerplate",
  "version": "0.1.0",
  "description": "",
  "main": "dev-server/index.js",
  "private": true,
  "scripts": {
    "prod": "npm run build && pm2 start ecosystem.config.js",
    "start": "npm run build && node build/server",
    "dev-local": "npm run build && cross-env NODE_ENV=development node dev-server/index.js",
    "build": "npm run build:hapi",
    "build:dev": "npm run build:hapi:client:dev && npm run build:hapi:ssr",
    "build:hapi": "npm run build:hapi:client && npm run build:hapi:ssr",
    "build:hapi:ssr": "npm run clean:hapi:ssr && cross-env NODE_ENV=development webpack --config ./webpack/webpack.config.ssr.js",
    "build:hapi:client": "npm run clean:hapi:client && workbox injectManifest workbox-config.js && cross-env NODE_ENV=development webpack --config ./webpack/webpack.config.web.js",
    "build:hapi:client:dev": "npm run clean:hapi:client && cross-env NODE_ENV=development webpack --config ./webpack/webpack.config.web.js",
    "clean:hapi": "npm run clean:hapi:ssr && clean:hapi:client",
    "clean:hapi:ssr": "rimraf build",
    "clean:hapi:client": "npm -v",
    "dev:hmr": "cross-env NODE_ENV=development webpack-dev-server --config ./webpack/webpack.config.dev.js --progress",
    "dev": "npm run build:dev && nodemon",
    "clean": "rimraf builds",
    "clean:ssr": "rimraf ssr",
    "test": "echo \"No test specified\" && exit 0",
    "export": "npm run build && next export",
    "pm2:sit": "pm2 start ecosystem.config.js --env sit",
    "pm2:qa": "pm2 start ecosystem.config.js --env qa",
    "pm2:stop": "pm2 stop",
    "pm2:delete": "pm2 delete",
    "pm2:restart": "pm2 restart",
    "pm2:logs": "pm2 logs",
    "pm2:list": "pm2 list"
  },
  "author": "Piyush",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
