    url('/static/fonts/icomoon/icomoon.ttf?o9vlji') format('truetype'),
    url('/static/fonts/icomoon/icomoon.woff?o9vlji') format('woff'),
    url('/static/fonts/icomoon/icomoon.svg?o9vlji#icomoon') format('svg')
  font-weight normal
  font-style normal

[class^="icon-"], [class*=" icon-"] 
/* use !important to prevent issues with browser extensions that change fonts */
